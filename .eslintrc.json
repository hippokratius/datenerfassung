  "ignorePatterns": [
    "datenmanagement/templates/datenmanagement/form.html",
    "datenmanagement/templates/datenmanagement/map.html"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "html",
    "django",
    "jinja2-processor"
  ],
  "rules": {
    "no-unused-vars": [
      "off"
    ]
  }
}
